c_cc_i86Linux2.6gcc3.4.3 = gcc
c_cc_flags_i86Linux2.6gcc3.4.3 = -m32
c_ld_i86Linux2.6gcc3.4.3 = gcc
c_ld_flags_i86Linux2.6gcc3.4.3 = -m32
cxx_cc_i86Linux2.6gcc3.4.3 = g++
cxx_cc_flags_i86Linux2.6gcc3.4.3 = -m32
cxx_ld_i86Linux2.6gcc3.4.3 = g++
cxx_ld_flags_i86Linux2.6gcc3.4.3 = -m32 -static-libgcc
syslibs_i86Linux2.6gcc3.4.3 = -ldl -lnsl -lm -lpthread
DEFINES_ARCH_SPECIFIC = -DRTI_UNIX -DRTI_LINUX


DEFINES = $(DEFINES_ARCH_SPECIFIC)  

INCLUDES = -I. -I$(NDDSHOME)/include -I$(NDDSHOME)/include/ndds

LIBS = -L$(NDDSHOME)/lib/i86Linux2.6gcc3.4.3 \
       -lnddscz -lnddscorez $(syslibs_i86Linux2.6gcc3.4.3)

CDRSOURCES    = tcs_kernel_Target.idl
COMMONSOURCES = tcs_kernel_Target.c tcs_kernel_TargetPlugin.c tcs_kernel_TargetSupport.c 
EXEC          = tcs_kernel_Target_subscriber \
		tcs_kernel_Target_publisher \
		tcs_kernel_Target_shmem_publisher \
		tcs_kernel_Target_shmem_tester
DIRECTORIES   = objs.dir objs/i86Linux2.6gcc3.4.3.dir
COMMONOBJS    = $(COMMONSOURCES:%.c=objs/i86Linux2.6gcc3.4.3/%.o)

# We actually stick the objects in a sub directory to keep your directory clean.
i86Linux2.6gcc3.4.3 : $(DIRECTORIES) $(COMMONOBJS) \
	       $(EXEC:%=objs/i86Linux2.6gcc3.4.3/%.o) \
	       $(EXEC:%=objs/i86Linux2.6gcc3.4.3/%.out)

objs/i86Linux2.6gcc3.4.3/%.out : objs/i86Linux2.6gcc3.4.3/%.o 
	$(c_ld_i86Linux2.6gcc3.4.3) $(c_ld_flags_i86Linux2.6gcc3.4.3) -o $(@:%.out=%) $(@:%.out=%.o) $(COMMONOBJS) $(LIBS)

objs/i86Linux2.6gcc3.4.3/%.o : %.c tcs_kernel_Target.h 
	$(c_cc_i86Linux2.6gcc3.4.3) $(c_cc_flags_i86Linux2.6gcc3.4.3)  -o $@ $(DEFINES) $(INCLUDES) -c $<

