struct power_Electrical {
  long Raw[16];
  float Calibrated[32];
};




struct power_TC {
  long Raw[16];
  float Calibrated[16];
};




struct power_UPSs {
  long Raw[16];
  float Calibrated[32];
};




struct power_command {
  long cmdID;
  string<16> device;
  string<16> operation;
  string<16> value;
  string<128> modifiers;
};




struct power_response {
  long cmdID;
  string<16> ack;
  string<16> error;
  long timeout;
  short repeat;
  short submits;
  string<64> result;
};




